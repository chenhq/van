html
  body
    .modal-header
      button.close(type='button', data-dismiss='modal', aria-hidden='true') &times;
      h3 About the {{help.type}} query
    .modal-body
      div(ng-include='queryHelpPath(help.type)')
    .modal-footer
      button.btn.btn-danger(type='button', ng-click='dismiss()') Close
